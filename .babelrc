{
  "presets": ["es2015", "react", "stage-0"],
  "plugins": [
    "transform-runtime", 
    "transform-export-extensions",
    [
      "import-rename", 
      { 
        "^(.*)\\.styl": "$1.css"
      }
    ],
    [
      "typecheck", 
      {
        "disable": 
        {
          "production": true
        }
      }
    ]
  ]
}
