{
  "name": "emsi-ui",
  "version": "1.1.11",
  "description": "UI components for use within Emsi",
  "main": "./public/bundle.js",
  "scripts": {
    "replace_imports": "replace '@import \"../../../assets/css/' '@import \"../' lib/**/*.css",
    "postcss": "postcss lib/**/*.css -r --use postcss-each postcss-for postcss-simple-vars postcss-color-function --local-plugins",
    "babel": "babel ./src/components --out-dir ./lib --ignore **/*-test.js",
    "copy_css": "cpx \"src/components/**/*.css\" lib && cpx \"assets/css/variables.css\" lib && cpx \"assets/css/mixins.css\" lib",
    "cleanup": "rm ./lib/mixins.css ./lib/variables.css",
    "postcssify": "find ./lib -type f -name '*.css' -exec ./node_modules/.bin/postcss -c postcss.json -r {} \\; && npm run cleanup",
    "build": "rm -rf lib/**/ && npm run babel && cpx \"./src/components/**/*.styl\" ./lib && stylus -I src lib/**/*.styl",
    "dev": "webpack-dev-server --config webpack.config.js --progress --profile --colors --hot",
    "storybook": "start-storybook -p 9001 -s ./public",
    "prepublish": "npm run build",
    "styleguide-server": "styleguidist server",
    "styleguide-build": "styleguidist build"
  },
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "lodash.uniqueid": "^4.0.1",
    "react": "^15.2.1",
    "react-addons-css-transition-group": "^15.3.1",
    "react-aria-menubutton": "^4.1.3",
    "react-aria-modal": "^2.3.1",
    "react-aria-tabpanel": "git://github.com/emsiengineering/react-aria-tabpanel",
    "react-dom": "^15.2.1"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.3.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-import-rename": "^1.0.1",
    "babel-plugin-tcomb": "^0.3.13",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-rename-import": "^2.0.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^3.0.1",
    "cpx": "^1.5.0",
    "css-loader": "^0.24.0",
    "dasherize": "^2.0.0",
    "eslint": "^3.3.1",
    "eslint-plugin-react": "^6.1.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "postcss": "^5.1.2",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-calc": "^5.3.1",
    "postcss-cli": "^2.6.0",
    "postcss-color-function": "^2.0.1",
    "postcss-composes": "^0.1.0",
    "postcss-css-variables": "^0.5.2",
    "postcss-cssnext": "^2.7.0",
    "postcss-custom-properties": "^5.0.1",
    "postcss-each": "^0.9.3",
    "postcss-easy-import": "^1.0.1",
    "postcss-for": "^2.1.1",
    "postcss-import": "^8.1.2",
    "postcss-less-engine": "^0.6.2",
    "postcss-loader": "^0.11.0",
    "postcss-mixins": "^5.0.1",
    "postcss-reporter": "^1.4.1",
    "postcss-simple-vars": "^3.0.0",
    "postcss-url": "^5.1.2",
    "react-css-modules": "^3.7.10",
    "react-hot-loader": "^1.3.0",
    "react-prop-types": "^0.3.2",
    "react-styleguidist": "^3.1.1",
    "replace": "^0.3.0",
    "style-loader": "^0.13.1",
    "stylelint": "^7.2.0",
    "stylelint-config-standard": "^13.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
